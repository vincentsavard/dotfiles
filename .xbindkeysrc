# Volume
"amixer set Master playback 1-"
    m:0x0 + c:122
    XF86AudioLowerVolume

"amixer set Master playback 1+"
    m:0x0 + c:123
    XF86AudioRaiseVolume

"amixer set Master toggle"
    m:0x0 + c:121
    XF86AudioMute

# previous, next, pause
"ncmpcpp next"
    m:0x0 + c:171
    XF86AudioNext

"ncmpcpp prev"
    m:0x0 + c:173
    XF86AudioPrev

"ncmpcpp toggle"
    m:0x0 + c:172
    XF86AudioPlay

"ncmpcpp toggle"
    m:0x0 + c:156
    XF86Launch1